{
	"compilerOptions": {
		"lib": ["ES2023"],
		"target": "ES2023",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"declaration": true,
		"outDir": "./dist",
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["src/**/*", "worker-*.js"]
}
